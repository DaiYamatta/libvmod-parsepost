varnishtest "POST request,ngtype,ngbody"

server s1 {
	rxreq

	txresp
} -start

varnish v1 -vcl+backend {
	import postparse from "${vmod_topbuild}/src/.libs/libvmod_postparse.so";

	sub vcl_recv {
		set req.http.x-run =  postparse.parse("X-TEST",true,"PREFIX",true,true);
	}
	sub vcl_deliver{
		set resp.http.x-run = req.http.x-run;
	}
} -start

client c1 {
	txreq -req POST -url "/" -body "hogehoge\r\nhogehoge"
	rxresp
	expect resp.http.x-run == "-4"

}


client c1 -run
